# a simple negation function definition and usage

# Input:

# Output:
# true
# false
# true

# Exit: 0

# Program:

begin
  bool neg(bool b) is
    return !b 
  end
  bool b = true ;
  println b ;
  b = call neg(b) ;
  println b ;
  b = call neg(b) ;
  b = call neg(b) ;
  b = call neg(b) ;
  println b
end
