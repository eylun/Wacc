# Program:

begin
  int plus(int x, int y) is
	return x + y
  end

  int w = 10;
  println w;
  int[] x = [1, 3, 5, 7, 9];
  int[] y = scan plus w x;
  println y[0];
  println y[1];
  println y[2];
  println y[3];
  println y[4];
  println y[5];
  println len y
end