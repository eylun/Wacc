# basic map implementation with square function

# Input:

# Output:
# 1
# 4
# 9
# 16
# 25
# 36
# 49
# 7

# Exit: 0

# Program:

begin
  int square(int x) is
	return x * x
  end

  int[] x = [1, 2, 3, 4, 5, 6, 7];
  int[] y = map square x;
  println y[0];
  println y[1];
  println y[2];
  println y[3];
  println y[4];
  println y[5];
  println y[6];
  println len y
end